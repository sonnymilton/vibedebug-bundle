{
    "name": "milton/vibedebug-bundle",
    "description": "Bundle for vibedebugging (integrated with symfony/ai)",
    "type": "symfony-bundle",
    "keywords": [
        "dev",
        "debugging"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Milton\\VibedebugBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Milton\\VibedebugBundle\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "league/commonmark": "^2.8",
        "symfony/ai-agent": "^0.1",
        "symfony/ai-chat": "^0.1.0",
        "symfony/dependency-injection": "^6.4 || ^7.0",
        "symfony/http-kernel": "^6.4 || ^7.0",
        "symfony/web-profiler-bundle": "^6.4 || ^7.0",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "friendsofphp/php-cs-fixer": "^3.87",
        "phpunit/phpunit": "^11.5"
    },
    "config": {
        "sort-packages": true
    },
    "authors": [
        {
            "name": "Nikolay Mikhaylov",
            "email": "sonny@milton.pro"
        }
    ]
}
